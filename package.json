{
    "name": "int-bot-v2",
    "version": "1.0.0",
    "description": "Discord bot to track inters.",
    "main": "build/index.js",
    "scripts": {
        "build": "tsc",
        "dev": "npm run server && npm run bot",
        "server": "npm run build && node --experimental-specifier-resolution=node build/server/server.js",
        "bot": "npm run build && node --experimental-specifier-resolution=node build/bot/index.js",
        "test": "jest --watchAll",
        "lint": "eslint --ext .js,.ts .",
        "format": "prettier --write \"**/*.+(js|ts|json)\"",
        "commandRegister": "npm run build && node --experimental-specifier-resolution=node build/bot/commands.js"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@discordjs/builders": "^0.12.0",
        "@discordjs/rest": "^0.3.0",
        "@prisma/client": "^3.9.2",
        "axios": "^0.25.0",
        "discord-api-types": "^0.27.2",
        "discord.js": "^13.6.0",
        "dotenv": "^16.0.0",
        "express": "^4.17.2",
        "node-schedule": "^2.1.0",
        "pg": "^8.7.3",
        "prisma": "^3.9.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.17",
        "@types/node-schedule": "^1.3.2",
        "@types/pg": "^8.6.4",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "eslint": "^8.9.0",
        "jest": "^27.5.1",
        "prettier": "^2.5.1",
        "ts-jest": "^27.1.3",
        "typescript": "^4.5.5"
    },
    "type": "module"
}
