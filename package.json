{
    "name": "int-bot-v2",
    "version": "1.0.0",
    "description": "Discord bot to track inters.",
    "main": "build/index.js",
    "scripts": {
        "build": "swc src/ --out-dir build/ --sync & tsc --pretty --skipLibCheck --noEmit",
        "server": "node --experimental-specifier-resolution=node build/server/server.js",
        "bot": "node --experimental-specifier-resolution=node build/bot/bot.js",
        "test": "jest --watchAll -i",
        "lint": "eslint --ext .js,.ts .",
        "format": "prettier --write \"**/*.+(js|ts|json)\"",
        "commandRegister": "npm run build && node --experimental-specifier-resolution=node build/bot/deployCommands.js"
    },
    "author": "Brian Koehler",
    "license": "ISC",
    "dependencies": {
        "@discordjs/builders": "^0.12.0",
        "@discordjs/rest": "^0.3.0",
        "@prisma/client": "^3.9.2",
        "axios": "^0.25.0",
        "discord-api-types": "^0.27.2",
        "discord.js": "^13.6.0",
        "dotenv": "^16.0.0",
        "express": "^4.17.2",
        "js-yaml": "^4.1.0",
        "node-schedule": "^2.1.0",
        "pg": "^8.7.3",
        "prisma": "^3.9.2"
    },
    "devDependencies": {
        "@swc/cli": "^0.1.55",
        "@swc/core": "^1.2.154",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/jest-when": "^3.5.0",
        "@types/js-yaml": "^4.0.5",
        "@types/node": "^17.0.17",
        "@types/node-schedule": "^1.3.2",
        "@types/pg": "^8.6.4",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.5.0",
        "jest": "^27.5.1",
        "jest-mock-extended": "^2.0.4",
        "jest-when": "^3.5.1",
        "prettier": "^2.5.1",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.3",
        "typescript": "^4.5.5"
    },
    "type": "module"
}
