{
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "decorators": true,
        "dynamicImport": true
      },
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "target": "es2022",
      "keepClassNames": true,
      "loose": true
    },
    "module": {
      "type": "es6",
      "strict": true,
      "strictMode": true,
      "lazy": false,
      "noInterop": true
    },
    "sourceMaps": "inline"
  }